{
  "version": 2,
  "projects": {
    "boilerplate": {
      "root": "apps/boilerplate",
      "sourceRoot": "apps/boilerplate/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nx/webpack:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "target": "node",
            "compiler": "tsc",
            "outputPath": "dist/apps/boilerplate",
            "main": "apps/boilerplate/src/main.ts",
            "tsConfig": "apps/boilerplate/tsconfig.app.json",
            "assets": [
              "apps/boilerplate/src/assets"
            ]
          }
        },
        "serve": {
          "executor": "@nx/js:node",
          "options": {
            "buildTarget": "boilerplate:build"
          }
        },
        "lint": {
          "executor": "@nx/eslint:lint",
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nx/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "apps/boilerplate/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": [
        "scope:app",
        "type:api"
      ]
    },
    "database": {
      "root": "libs/database",
      "sourceRoot": "libs/database/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nx/js:tsc",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/database",
            "main": "libs/database/src/index.ts",
            "tsConfig": "libs/database/tsconfig.lib.json"
          }
        },
        "lint": {
          "executor": "@nx/eslint:lint",
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nx/jest:jest",
          "outputs": [
            "{workspaceRoot}/coverage/{projectRoot}"
          ],
          "options": {
            "jestConfig": "libs/database/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": [
        "scope:shared",
        "type:data"
      ]
    },
